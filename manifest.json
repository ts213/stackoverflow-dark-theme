{
  "manifest_version": 2,
  "name": "StackOverflow dark theme",
  "version": "2.0.1",
  "description": "StackOverflow dark theme without being logged in",
  "permissions": [
    "http://stackoverflow.com/*",
    "https://stackoverflow.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.stackoverflow.com/*",
        "https://*.stackoverflow.com/*"
      ],
     "exclude_matches": ["https://meta.stackoverflow.com/*"],
      "css": ["style.css"],
      "run_at": "document_start"
    }
  ]
}
